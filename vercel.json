{
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**/*",
        "use": "@vercel/static"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api"
      },
      {
        "source": "/(.*)",
        "destination": "/dist/index.html"
      }
    ]
  }